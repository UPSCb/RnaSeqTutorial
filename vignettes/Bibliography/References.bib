@Article{	  Cock:2009p249,
  author	= {Peter J A Cock and Christopher J Fields and Naohisa Goto
		  and Michael L Heuer and Peter M Rice},
  journal	= {Nucleic Acids Research},
  title		= {The Sanger FASTQ file format for sequences with quality
		  scores, and the Solexa/Illumina FASTQ variants},
  abstract	= {FASTQ has emerged as a common file format for sharing
		  sequencing read data combining both the sequence and an
		  associated per base quality score, despite lacking any
		  formal definition to date, and existing in at least three
		  incompatible variants. This article defines the FASTQ
		  format, covering the original Sanger standard, the
		  Solexa/Illumina variants and conversion between them, based
		  on publicly available information such as the MAQ
		  documentation and conventions recently agreed by the Open
		  Bioinformatics Foundation projects Biopython, BioPerl,
		  BioRuby, BioJava and EMBOSS. Being an open access
		  publication, it is hoped that this description, with the
		  example files provided as Supplementary Data, will serve in
